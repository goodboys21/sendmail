{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/send-email", "dest": "/server.js" },
    { "src": "/auto", "dest": "/server.js" },
    { "src": "/send2", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
